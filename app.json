{
  "expo": {
    "name": "한줄뱅크",
    "slug": "onelinebank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1277cb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.mugohan.onelinebank",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app accesses to photo library to upload your profile photo when you are signing up or updating the profile.",
        "NSFaceIDUsageDescription": "The app transfer money by FaceID"
      }
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.mugohan.onelinebank",
      "versionCode": 1,
      "permissions": ["CAMERA", "USE_BIOMETIRC", "USE_FINGERPRINT"],
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#1277cb",
        "foregroundImage": "./assets/adaptive-icon.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
